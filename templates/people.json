{
  "name": {
    "firstName": "first_name",
    "lastName": "last_name"
  },
  "age": {"attr" : "random_int", "args": [16, 106]},
  "sex": {"attr" : "random_element", "args": [["M", "F"]]},
  "single": "boolean",
  "email": "free_email",
  "phone": "phone_number",
  "street": "street_address",
  "state": "state",
  "postcode": {
    "frmt": "ZIP: {postcode}",
    "attr": "postcode"
  },
  "tags": [
    "word",
    {
      "attr": "day_of_week",
      "postprocess": [
        "lower",
        {
          "attr": "encode",
          "args": ["rot13"]
        }
      ]},
    "word",
    "word"
  ],
  "times": {
    "createdAt": {
      "attr": "date_time_between",
      "args": ["-1y", "-1m"]
    },
    "updatedAt": {
      "attr": "date_time_between",
      "args": ["-1m", "now"]
    }
  }
}
